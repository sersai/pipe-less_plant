##adding new msg
#CMakeLists.txt
find_package(catkin REQUIRED COMPONENTS
	rospy
	std_msgs
	message_generation # (1) add this line
)
add_message_files(
 FILES # (2) uncomment this line
 rfid_data.msg # (3) add the .msg file which must be under msg folder
generate_messages( # (4) uncomment whole of generate_messages
 DEPENDENCIES
 std_msgs
)
catkin_package( # (5) uncomment whole of catkin_package and add (6)
 INCLUDE_DIRS
 LIBRARIES "package name"
 CATKIN_DEPENDS rospy std_msgs
 DEPENDS system_lib
 CATKIN_DEPENDS message_runtime # (6) add this line
)


#scans the router for devices 
$nmap -sP 192.168.0.0/24 
